{
  "name": "rslang",
  "version": "0.0.0",
  "scripts": {
    "dev": "run-p -l -n restart:server dev:client",
    "dev:client": "vite",
    "start": "npm run restart:server",
    "restart:server": "run-s stop:server start:server",
    "start:server": "npm start --prefix server",
    "start:server:dev": "npm run start:dev --prefix server",
    "build": "run-p -l -n build:server build:client",
    "build:client": "tsc && vite build --base=app",
    "build:server": "npm install --prefix server",
    "stop:server": "npm run stop --prefix server",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:watch": "esw . --ext .ts,.tsx -w",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.3.1",
    "axios": "^0.25.0",
    "formik": "^2.2.9",
    "i18next": "^21.6.10",
    "i18next-browser-languagedetector": "^6.1.3",
    "i18next-http-backend": "^1.3.2",
    "notistack": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "^6.4.16",
    "@storybook/addon-essentials": "^6.4.16",
    "@storybook/addon-links": "^6.4.16",
    "@storybook/react": "^6.4.16",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-react": "^1.0.7",
    "babel-loader": "^8.2.3",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-watch": "^8.0.0",
    "global-jsdom": "^8.4.0",
    "gsap": "^3.9.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jsdom": "^19.0.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-eslint": "^1.3.0"
  }
